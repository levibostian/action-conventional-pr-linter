{
  "name": "@levibostian/action-semantic-pr",
  "version": "0.1.0",
  "scripts": {
    "test": "npx jest",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "lint": "npx eslint --fix --ext ts,js . && npx tsc --noEmit",
    "compile": "rm -rf dist/ && npx ncc build app/index.ts --source-map -o dist && cp -r app/assets/ dist/assets/",
    "prepare": "husky install"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,json,css}": [
      "npm run format"
    ]
  },
  "prettier": "@levibostian/prettier-config",
  "devDependencies": {
    "@levibostian/eslint-config-node": "^1.3.1",
    "@levibostian/jest-config": "^1.0.1",
    "@levibostian/prettier-config": "^1.0.0",
    "@levibostian/tsconfigs": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/string-format": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.7.0",
    "eslint-plugin-jest": "^25.7.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/lint": "^16.0.0",
    "@commitlint/load": "^16.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "cathy": "^1.0.0",
    "string-format": "^2.0.0"
  }
}
